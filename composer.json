{
    "name": "drewlabs/google-authenticator",
    "type": "library",
    "description": "Library to integrate Google Authenticator into a PHP project",
    "keywords": [
        "google authenticator"
    ],
    "homepage": "https://github.com/azandrew-sidoine/GoogleAuthenticator",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@gmail.com"
        },
        {
            "name": "Christian Stocker",
            "email": "me@chregu.tv"
        },
        {
            "name": "Andre DeMarre",
            "homepage": "http://www.devnetwork.net/viewtopic.php?f=50&t=94989"
        },
        {
            "name": "Sidoine Azandrew",
            "email": "azandrewdevelopper@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.1.8"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Google\\Authenticator\\": "src/",
            "Drewlabs\\GoogleAuthenticator\\": "src/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Google\\Authenticator\\Tests\\": "tests/",
            "Drewlabs\\GoogleAuthenticator\\Tests\\": "tests/"
        }
    }
}
